					 var num=0;
					 $('#lbBrands ul').append($('#lbBrands ul').html());
					 $('#lbBrands .btn_next').click(function(){
					  num++;
					  if(num>lis){
					   num=1;
					   $('#lbBrands ul').css('left',0)
					  }
					  $('#lbBrands ul').stop().animate({left:-num*100},500);
					 })
					 $('#lbBrands .btn_prev').click(function(e) {
					  num--
					  if(num<0){
					   num=lis-1;
					   $('#lbBrands ul').css('left',-lis*100)
					  }
					  $('#lbBrands ul').stop().animate({left:-num*100},500)
