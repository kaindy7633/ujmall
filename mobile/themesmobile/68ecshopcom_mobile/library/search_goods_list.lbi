        <div class="proSales"> 销量:<em>{$goods.wap_count}</em> </div>
          <!--{if $goods.promote_price neq "" } -->
        <div class="proIcons"> <img src="themesmobile/68ecshopcom_mobile/img/goods_new.png" /> </div>
        <!--{/if}-->
      </div>
    </div>
  </div>
{/foreach}
</div>
